                                <div class="col-md-6">
                                    <div class="form-group">
                                        @Html.Label("Receive Date", new {@class = "control-label col-md-4"})
                                        <div class="col-md-8">
                                            @Html.TextBoxFor(model => model.HwQcAssignCustomMasterModel.HwQcAssignDate, new {@Class = "form-control", @readonly = "true"})
                                        </div>
                                    </div>
                                </div>
                            </div>



                        </div>*@
                    </div>
                </div>
            </div>
        </div>
    </div>
}

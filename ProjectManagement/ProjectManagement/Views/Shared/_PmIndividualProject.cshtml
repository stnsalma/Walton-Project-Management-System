            '<input name = "" type="text"  />' +
            '<input name = "" type="text"  />'
    }

    

    function GetDynamicRowOfInput(counter) {
        return '<div class="col-lg-5"></br><input class="form-control" type="text" name="otherspath['+counter+']"/></div>' +
            '<div class="col-lg-4"></br><input class="form-control" type="text" name="otherssettings[0]" /></div></div></br><div class="col-lg-1"><button  class=" btn btn-danger remove_textBox">Remove</button></div></br>'



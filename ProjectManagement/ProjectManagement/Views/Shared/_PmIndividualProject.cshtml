                    document.getElementById("TextBoxContainer").innerHTML = html;

                }

            } else {

                html += "";
                document.getElementById("TextBoxContainer").innerHTML = html;
            }


        });        


        $(add_button).click(function(e) { //on add input button click
            e.preventDefault();
            x++;
            $(wrapper).append(GetDynamicRowOfInput(x)); //add input box

            x++; //text box increment
    
           
        });
        

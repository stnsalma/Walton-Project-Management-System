@model ProjectManagement.ViewModels.MaterialWastageReportTopSheetViewModel


  


<body>
    <div class="portlet box default">

        <div class="portlet-body form">
            <div class="row">
                <div class="col-lg-12">

                    <div class="col-lg-8">
                        <table class="" style="align-content: center; width: 100%">
                            <tr>
                                <td style="width: 30%">
                                    <img style="margin-top: 60px" src="@Url.Content("~/assets/layouts/layout4/img/walton_logo.png")" alt="logo" class="logo-default" width="100" height="60" />
                                </td>
                                <td style="width: 70%">
                                    <table>
                                        <tr><td style="text-align: center; font-weight: bold">@Html.DisplayFor(model => model.CompanyName)</td></tr>
                                        <tr><td style="text-align: center; font-weight: bold">@Html.DisplayFor(model => model.UnitName)</td></tr>
                                        <tr><td style="text-align: center; font-weight: bold">@Html.DisplayFor(model => model.Address)</td></tr>
                                        <tr><td style="text-align: center; font-weight: bold">@Html.DisplayFor(model => model.Adderes2)</td></tr>
                                        <tr><td style="text-align: center; font-weight: bold">@Html.DisplayFor(model => model.MonthName)</td></tr>
                                    </table>
                                </td>
                            </tr>
                        </table>
                    </div>



                    @*<div class="col-lg-3">
                            <img src="@Url.Content("~/assets/layouts/layout4/img/logo-light.png")" alt="logo" class="logo-default" />
                        </div>
                        <div class="col-lg-9">
                            <div class="row"><strong>@Html.DisplayFor(model => model.CompanyName)</strong></div>
                            <div class="row"><strong>@Html.DisplayFor(model => model.UnitName)</strong></div>
                            <div class="row"><strong>@Html.DisplayFor(model => model.Address)</strong></div>
                            <div class="row"><strong>@Html.DisplayFor(model => model.Adderes2)</strong></div>
                            <div class="row"><strong>@Html.DisplayFor(model => model.MonthName)</strong></div>
                        </div>*@
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <table class="table table-striped table-scrollable table-bordered table-hover" id="sample_1">
                        <thead>
                            <tr>
                                <th>Particulars</th>
                                <th>Value</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model.Particulars)
                            {
                                <tr>

                                    <td>@item.Particular</td>
                                    <td>@item.PriceValue</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">

                    <table>
                        <tr>
                            <td>
                                <table class="table table-bordered">
                                    <tr><td><strong>Created By</strong></td></tr>
                                    @foreach (var i in Model.CreatorList)
                                    {
                                        <tr><td>@i</td></tr>
                                    }
                                </table>
                            </td>
                            <td>
                                <table class="table table-bordered">
                                    <tr><td><strong>Incharge Approval By</strong></td></tr>
                                    @foreach (var i in Model.InChargeList)
                                    {
                                        <tr><td>@i</td></tr>
                                    }
                                </table>
                            </td>
                            <td>
                                <table class="table table-bordered">
                                    <tr><td><strong>Deputy Approval By</strong></td></tr>
                                    @foreach (var i in Model.DeputyCooList)
                                    {
                                        <tr><td>@i</td></tr>
                                    }
                                </table>
                            </td>
                            <td>
                                <table class="table table-bordered">
                                    <tr><td><strong>COO Approval By</strong></td></tr>
                                    @foreach (var i in Model.CooList)
                                    {
                                        <tr><td>@i</td></tr>
                                    }
                                </table>
                            </td>
                            <td>
                                <table class="table table-bordered">
                                    <tr><td><strong>Management Approval By</strong></td></tr>
                                    @foreach (var i in Model.ApprovalList)
                                    {
                                        <tr><td>@i</td></tr>
                                    }
                                </table>
                            </td>
                        </tr>
                    </table>




                </div>
            </div>

        </div>
    </div>
    
    <footer id="printFooter">
        
    </footer>
</body>





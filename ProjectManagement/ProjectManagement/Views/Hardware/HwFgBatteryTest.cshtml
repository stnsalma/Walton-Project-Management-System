                                            @Html.TextBoxFor(model => model.HwFgBatteryTestMasterModel.TestEnvironment_Temperature, new { @Class = "form-control", placeholder = "°C" })
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-6">
                                    <div class="form-group">
                                        @Html.Label("Test Environment Humidity", new { @class = "control-label col-md-4" })
                                        <div class="col-md-8">
                                            @Html.TextBoxFor(model => model.HwFgBatteryTestMasterModel.TestEnvironment_Humidity, new { @Class = "form-control", placeholder = "%" })
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <br />

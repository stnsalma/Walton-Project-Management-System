                                <div class="col-md-6">
                                    <div class="form-group">
                                        @Html.Label("Test Item", new { @class = "control-label col-md-4" })
                                        <div class="col-md-8">
                                            @Html.TextBoxFor(model => model.HwFgBatteryTestMasterModel.TestItem, new { @Class = "form-control" })
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-6">
                                    <div class="form-group">
                                        @Html.Label("Test Date", new { @class = "control-label col-md-4" })
                                        <div class="col-md-8">
                                            @*@Html.TextBoxFor(model => model.HwFgBatteryTestMasterModel.TestDate, new { @Class = "form-control" })*@
                                            @Html.TextBoxFor(model => model.HwFgBatteryTestMasterModel.TestDate, new { @class = "form-control date_picker" })
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        @Html.Label("Max Charge Voltage", new { @class = "control-label col-md-4" })
                                        <div class="col-md-8">
                                            @Html.TextBoxFor(model => model.HwFgBatteryTestMasterModel.MaxChargeVoltage, new { @Class = "form-control" })
                                        </div>
                                    </div>
                                </div>


                            </div>
                            <br />


                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <div class="col-md-10 col-md-offset-2">
                                            @if (@ViewBag.hwFgBatteryTestMasterId == null)
                                            {
                                                <input type="submit" value="Save" class="btn btn-default" id="btnsave" />
                                            }
                                            else
                                            {
                                                <input type="submit" value="Update" class="btn btn-default" id="btnupdate" />
                                            }
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>



}




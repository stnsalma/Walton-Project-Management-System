@model IEnumerable<ProjectManagement.ViewModels.Commercial.VmPendingTac>

@{
    ViewBag.Title = "PendingTacList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-md-12">
        <!-- BEGIN EXAMPLE TABLE PORTLET-->
        <div class="portlet light bordered">
            <div class="portlet-title">
                <div class="col-lg-2">
                    <div class="caption font-dark">
                        <i class="icon-settings font-dark"></i>
                        <span class="caption-subject bold uppercase">BABT List</span>
                    </div>
                    <div class="tools"> </div>
                </div>
                @*<div class="col-lg-2">
                    @Html.ActionLink("Create New", "Babt")
                </div>*@

            </div>
            <div class="portlet-body">
                <div class="col-lg-12">
                    @{
                        if (TempData["message"] != null)
                        {
                            var msgType = TempData["messageType"];
                            var msg = TempData["message"];
                            if ((int)msgType == 1)
                            {
                                <div class="alert alert-success alert-dismissable">
                                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true"></button>
                                    <strong>Message:</strong> @msg
                                </div>
                            }
                            else if ((int)msgType == 2)
                            {
                                <div class="alert alert-danger alert-dismissable">
                                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true"></button>
                                    <strong>Message:</strong> @msg
                                </div>
                            }
                            else if ((int)msgType == 3)
                            {
                                <div class="alert alert-info alert-dismissable">
                                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true"></button>
                                    <strong>Message:</strong> @msg
                                </div>
                            }
                        }
                    }
                </div>
                <table class="table table-striped table-scrollable table-bordered dtTable" id="sample_1">

                    <thead>
                        <tr>
                            <th>@Html.DisplayNameFor(model => model.ProjectName)</th>
                            @*<th>@Html.DisplayNameFor(model => model.DefinedModel)</th>*@
                            <th>Action</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.ProjectName)
                                </td>
                                @*<td>
                                    @Html.DisplayFor(modelItem => item.DefinedModel)
                                </td>*@
                                <td>
                                    @Html.ActionLink("Create", "Babt", new { projectName = item.ProjectName })
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>

            </div>
        </div>
    </div>
</div>

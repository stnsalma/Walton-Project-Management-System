                                {
                                    foreach (var item in ViewBag.GetProjectSpec)
                                    {
                                <tr class="trClsforSize" style="text-align: center">
                                    <td style="text-align: left">@item.ProjectMasterId</td>
                                    <td style="text-align: left">@item.ProjectName</td>
                                    <td style="text-align: left">@item.ProjectModel</td>
                                    <td style="text-align: left">@item.Orders</td>
                                    <td style="text-align: left">@item.IsActiveStatus</td>
                                    <td style="text-align: left">
                                        @if (@item.ProjectCreationDate != null)
                                                {
                                                @String.Format("{0:yyyy-MM-dd}", @item.ProjectCreationDate)
                                                }
                                    </td>
                                    <td style="text-align: left">
                                        @item.SupplierName

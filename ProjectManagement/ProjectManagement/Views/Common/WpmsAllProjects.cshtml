@using System.Diagnostics
@using System.Web.Mvc.Html
@model ProjectManagement.ViewModels.Common.VmWpmsAllProjectDetails

<style>
    .page-header-fixed .page-container {
        margin-top: 0px;
    }

    .navbar-fixed-top {
        position: relative;
    }

    #loading {
        width: 100%;
        height: 100%;
        top: 70px;
        left: 0;
        position: fixed;
        display: block;
        opacity: 0.7;
        background-color: #fff;
        z-index: 999;
        text-align: center;
    }
</style>
@using (@Html.BeginForm("WpmsAllProjects", "Common", FormMethod.Post, new { enctype = "multipart/form-data", id = "form1" }))
{
    <div class="row">
        <div class="col-lg-12">
            <div class="portlet box green-seagreen">
                <div class="portlet-title">
                    <div class="caption">
                        All WPMS Project List
                    </div>
                </div>
                <div class="portlet-body form">
                    <br />
                    <br />
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="col-lg-2" style="color: brown;"></div>
                            <div class="col-lg-8" style="color: brown;">
                                <div class="col-lg-3" style="color: brown; font-weight: bold">Select Project</div>
                                <div class="col-lg-9">
                                    @if (ViewBag.GetAllModels != null)
                                    {
                                    @Html.DropDownListFor(model => model.ProjectName, new SelectList(ViewBag.GetAllModels, "Value", "Text"), "ALL", new { @class = "dropForProject form-control ddChosen", @id = "dropForProject", @onchange = "LoadOrders()", Value = @Model.ProjectName })
                                    }

                                </div>
                            </div>
                            <div class="col-lg-2" style="color: brown;"></div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="col-lg-2" style="color: brown;"></div>
                            <div class="col-lg-8" style="color: brown;">
                                <div class="col-lg-3" style="color: brown; font-weight: bold">Orders</div>
                                <div class="col-lg-9">
                                    @Html.DropDownListFor(model => model.Orders, new SelectList(ViewBag.ProjectOrderLists, "Value", "Text"), "ALL", new { @class = "ProjectMasterId2 form-control", @id = "ProjectMasterId2" })
                                </div>
                            </div>
                            <div class="col-lg-4" style="color: brown;"></div>
                        </div>
                    </div>

                    <br />
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="col-lg-2" style="color: brown;"></div>

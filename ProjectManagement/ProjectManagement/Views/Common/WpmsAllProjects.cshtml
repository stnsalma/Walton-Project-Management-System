                                        <input type="text" value="@item.Rom" class="Rom" id="Rom" style="display: none" />
                                        <input type="text" value="@item.SupplierTrustLevel" class="SupplierTrustLevel" id="SupplierTrustLevel" style="display: none" />
                                        <input type="text" value="@item.OsName" class="OsName" id="OsName" style="display: none" />


                                        <input type="text" value="@item.OsVersion" class="OsVersion" id="OsVersion" style="display: none" />
                                        <input type="text" value="@item.DisplaySize" class="DisplaySize" id="DisplaySize" style="display: none" />

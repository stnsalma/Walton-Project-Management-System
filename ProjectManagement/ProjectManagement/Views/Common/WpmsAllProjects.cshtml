                                        <input type="text" value="@item.ProjectNameForScreening" class="ProjectNameForScreening" id="ProjectNameForScreening" style="display: none" />
                                        <input type="text" value="@item.PcbaFinalVendor" class="PcbaFinalVendor" id="PcbaFinalVendor" style="display: none" />

                                        <input type="text" value="@item.PcbaVendorName" class="PcbaVendorName" id="PcbaVendorName" style="display: none" />
                                        <input type="text" value="@item.DisplayResulution" class="DisplayResulution" id="DisplayResulution" style="display: none" />
                                        <input type="text" value="@item.Color" class="Color" id="Color" style="display: none" />
                                        <input type="text" value="@item.BatteryType" class="BatteryType" id="BatteryType" style="display: none" />
                                        <input type="text" value="@item.BatteryRating" class="BatteryRating" id="BatteryRating" style="display: none" />
                                        <input type="text" value="@item.DisplayResulution" class="SecondGen" id="SecondGen" style="display: none" />

                                        <input type="text" value="@item.FlashLight" class="FlashLight" id="FlashLight" style="display: none" />
                                        <input type="text" value="@item.BatteryCoverLogoType" class="BatteryCoverLogoType" id="BatteryCoverLogoType" style="display: none" />
                                        <input type="text" value="@item.SpecialSensor" class="SpecialSensor" id="SpecialSensor" style="display: none" />

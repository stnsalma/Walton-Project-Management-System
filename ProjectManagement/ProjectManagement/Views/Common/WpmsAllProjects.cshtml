                                        <input type="text" value="@item.ChipsetCore" class="ChipsetCore" id="ChipsetCore" style="display: none" />

                                        <input type="text" value="@item.ChipsetBit" class="ChipsetBit" id="ChipsetBit" style="display: none" />
                                        <input type="text" value="@item.ChipsetFrequency" class="ChipsetFrequency" id="ChipsetFrequency" style="display: none" />
                                        <input type="text" value="@item.ChipsetName" class="ChipsetName" id="ChipsetName" style="display: none" />
                                        <input type="text" value="@item.CpuName" class="CpuName" id="CpuName" style="display: none" />
                                        <input type="text" value="@item.FrontCamBsi" class="FrontCamBsi" id="FrontCamBsi" style="display: none" />
                                        <input type="text" value="@item.FrontCamSensor" class="FrontCamSensor" id="FrontCamSensor" style="display: none" />
                                    </td>
                                </tr>
                                    }
                                }

                            </tbody>
                        </table>
                    </div>
                    <br />
                </div>
            </div>
        </div>
    </div>
}


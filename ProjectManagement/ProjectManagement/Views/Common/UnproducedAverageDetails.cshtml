@model IEnumerable<ProjectManagement.Models.Common.rbsBarCodeInv>
    @{
        ViewBag.Title = "UnproducedAverageDetails";
        Layout = "~/Views/Shared/_Layout.cshtml";
    }

    <div class="portlet light">
        <div class="portlet-title">
            <div class="caption">
                Unproduced Average Details
            </div>
        </div>
        <div class="portlet-body">
            <div class="table-responsive">
                <table class="table table-bordered">
                    <thead>
                        <tr>
                            <th class="text-center">Project Model</th>
                            <th class="text-center">Order No</th>
                            <th class="text-center">Order Quantity</th>
                            <th class="text-center">Produced</th>
                            <th class="text-center">Unproduced</th>
                            <th class="text-center">Unproduced(%)</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var v in Model)
                        {
                            <tr>
                                <td class="text-center">@v.ProjectModel</td>
                                <td class="text-center">@v.Order_No</td>
                                <td class="text-center">@v.OrderQuantity</td>
                                <td class="text-center">@v.Produced</td>
                                <td class="text-center">@v.UnProduced</td>
                                <td class="text-center">@v.UnproducedPercentage%</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>


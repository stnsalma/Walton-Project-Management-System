<div class="note note-success">
    <p style="color: green;font-size: 20px;"> Software Qc Incentive </p>
</div>

<div class="portlet box green">
    <div class="portlet-title">
        <div class="caption">
            <i class="fa fa-picture"></i>Select All Field
        </div>
    </div>
    <div class="portlet-body" style="display: block;">
        <div class="row">
            <div class="col-lg-12">
                <div class="col-lg-6">
                    <div class="col-lg-3" style="text-align: right">
                        Select Month
                    </div>
                    <div class="col-lg-9" style="text-align: left">

                        @if (ViewBag.ddlMonths != null)
                        {
                            @Html.DropDownListFor(model => model.Month, new SelectList(ViewBag.ddlMonths, "Value", "Text"), new { @class = "DDDMonthCls1 form-control", @id = "DDDMonth" })

                        }
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="col-lg-5" style="text-align: right">
                        Select Year
                    </div>
                    <div class="col-lg-7" style="text-align: left">

                        @if (ViewBag.ddlYears != null)
                        {
                            @Html.DropDownListFor(model => model.Year, new SelectList(ViewBag.ddlYears, "Value", "Text"), new { @class = "DDDYearCls1 form-control", @id = "DDDYear" })

                        }
                    </div>
                </div>
            </div>
            <br />
            <br />
            <div class="col-lg-12">
                <div class="col-lg-6">
                    <div class="col-lg-3" style="text-align: right">
                        Role Type
                    </div>
                    <div class="col-lg-9" style="text-align: left">

                        @if (ViewBag.ddlYears != null)
                        {
                            @Html.DropDownListFor(model => model.RoleName, new SelectList(ViewBag.ddlRoles, "Value", "Text"), new { @class = "DDDRoleCls form-control", @id = "DDDRole" })

                        }
                    </div>
                </div>
                <div class="col-lg-6" style="text-align: center">
                    <div class="col-lg-5" style="text-align: right">
                        Select Person
                    </div>
                    <div class="col-lg-7" style="text-align: left">
                        @if (ViewBag.ddlUsers != null)
                        {
                            @Html.DropDownListFor(model => model.EmployeeCode, new SelectList(ViewBag.ddlUsers, "Value", "Text"), new { @class = "dropForPerson form-control", @id = "sltPerson" })

                        }
                    </div>
                </div>
            </div>
            <br /><br />
            <div class="col-lg-12">
                <div class="col-lg-3"> </div>
                <div class="form-actions">
                    <input type="button" id="btnforLoad" class="btn sbold uppercase btn-outline blue" value="Load" onclick="location.href = '@Url.Action("All_QcMembersMonthlyIncentive", "Software")?months=' + $('option:selected', $('#DDDMonth')).val() + '&years=' + $('option:selected', $('#DDDYear')).val() + '&roles=' + $('option:selected', $('#DDDRole')).val() + '&persons=' + $('option:selected', $('#sltPerson')).val() " />

                </div>
                <div class="col-lg-3"> </div>
            </div>

        </div>

    </div>
</div>
<br />
@if (TempData["Message2"] != null)
{
